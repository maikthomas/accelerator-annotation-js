{
  "name": "opentok-annotation",
  "version": "2.0.52",
  "description": "OpenTok annotation accelerator pack",
  "main": "dist/opentok-annotation.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build-css": "npm-css css/annotation.css -o dist/annotation.css",
    "build-js": "concat -o dist/opentok-annotation.js src/* && uglifyjs dist/opentok-annotation.js -o dist/opentok-annotation.min.js",
    "build-images": "cp -r images/ dist/images/",
    "build": "mkdir -p dist && npm run build-js & npm run build-css & npm run build-images",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "opentok",
    "annotation",
    "annotations",
    "opentok-acc-pack",
    "screen-sharing",
    "audio/video"
  ],
  "author": "adrice727@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/opentok/accelerator-annotation-js"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "concat": "^3.0.0",
    "karma": "^0.13.10",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-requirejs": "^0.2.2",
    "karma-sinon": "^1.0.4",
    "merge-stream": "^1.0.0",
    "mocha": "^2.3.3",
    "npm-css": "^0.2.3",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.0",
    "uglify-js": "^2.8.15",
    "yuidocjs": "^0.9.0"
  },
  "dependencies": {
    "jquery": "^3.1.0",
    "opentok-solutions-logging": "^1.0.6",
    "underscore": "^1.8.3"
  }
}
